{
  "name": "express-app-starter",
  "version": "0.0.1",
  "author": "bradford lamson-scribner",
  "email": "brad.lamson@gmail.com",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nodemon app.js --exec babel-node",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && babel . -d dist --ignore node_modules",
    "serve": "node app.js",
    "test": "NODE_ENV=test mocha -R nyan --compilers js:babel-register $(find test -name '*.test.js')"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chai": "^4.2.0",
    "cookie-parser": "^1.4.3",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "http-errors": "^1.7.1",
    "mocha": "^5.2.0",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "nodemon": "^1.18.9"
  }
}
